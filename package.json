{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "Node Bolilerplate with TypeORM",
  "main": "dist/app.js",
  "scripts": {
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "start": "tsc && nodemon dist/server.js",
    "serve": "nodemon dist/server.js",
    "start:dev": "cross-env NODE_ENV=development && npm run serve",
    "start:prod": "cross-env NODE_ENV=production && npm run serve",
    "build:prod": "rm -rf dist && webpack --env.NODE_ENV=production --config webpack.config.prod.js",
    "build:dev": "rm -rf dist && webpack --env.NODE_ENV=development --config webpack.config.dev.js",
    "watch": "webpack --watch --config webpack.config.dev.js",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "engines": {
    "node": ">=14.15.2",
    "npm": ">=6.14.9"
  },
  "author": "Anjali Sahu",
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.5",
    "dotenv-webpack": "^3.0.0",
    "ts-loader": "^8.0.4",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.4",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29"
  }
}
